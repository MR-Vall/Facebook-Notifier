  {
      "manifest_version": 3,
      "name": "Facebook Notifier",
      "version": "1.0",
      "description": "This extension will notify you when an item has been updated from marketplace.",
      "permissions": ["storage", "tabs"],
      "background": {
        "service_worker": "background.js"
      },
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "Pictures/facebook16.png",
          "48": "Pictures/facebook48.png",
          "128": "Pictures/facebook128.png"
        }
      },
      "icons": {
        "16": "Pictures/facebook16.png",
        "48": "Pictures/facebook48.png",
        "128": "Pictures/facebook128.png"
      }
  }   